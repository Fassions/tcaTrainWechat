/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
  width: 100%;height: 100%;background: #f2f2f2;
}
.page_view{
  width: 100%;height: 100%;
}



/*遮罩层*/
.commodityScreenClass {
    width: 100%;height: 100%;position: fixed;top:0;background: #000;opacity: 0.4;overflow: hidden;z-index: 10;color: #fff;
}



.overflow_one{
  overflow: hidden;text-overflow:ellipsis;white-space: nowrap;
}
.overflow_three{
    display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 3;overflow: hidden;
}

.hover_class{
  background: #eee !important;
}
.hover_aaa_class{
  background: #aaa;
}

/**50acfb */

/*空数据展示样式*/
.gapViewClass{
  width: 100%;height: 100%;
}
.gapViewClass image{
  width: 100px;height: 100px;margin-left: calc(50% - 50px);margin-top: 50%;float: left;
}
.gapViewClass text{
  width: 100%;height: 30px;font-size: 14px;color: #666;text-align: center;line-height: 30px;display: block;float: left;margin-top: 10px;
}


.not_view{
  width: 100%;height: 100%;
}
.not_view image{
  width: 30%;margin: 0 auto;
}
.not_view view{
  margin: 0 auto;font-size: 32rpx;color: #999;pading: 10rpx 0;
}


/** wrui */

.wrui-view_display_flex_row{
position:relative;display:-webkit-flex;display:flex;-webkit-box-align:center;flex-direction: row;justify-content: center;
}



.wrui-view_display_flex_column{
position:relative;display:-webkit-flex;display:flex;-webkit-box-align:center;flex-direction: column;justify-content: center;
}

.wrui-view_flex_1{
  flex: 1
}


.wrui-view_overflow_h{
  overflow: hidden;position: relative;
}
.wrui-view_overflow_a{
  overflow: auto;position: relative;
}









.weui-loadmore{
  width:65%;margin:1.5em auto;line-height:1.6em;font-size:14px;text-align:center;
}
.weui-loadmore__tips{
  display:inline-block;vertical-align:middle;
}
.weui-loadmore_line{
  border-top:1px solid #e5e5e5;margin-top:2.4em;
}
.weui-loadmore__tips_in-line{
  position:relative;top:-.9em;padding:0 .55em;background-color:#fff;color:#999;
}
.weui-loadmore__tips_in-dot{
  position:relative;padding:0 .16em;width:4px;height:1.6em;
}
.weui-loadmore__tips_in-dot:before{
  content:" ";position:absolute;top:50%;left:50%;margin-top:-1px;margin-left:-2px;width:4px;height:4px;border-radius:50%;background-color:#e5e5e5
}
